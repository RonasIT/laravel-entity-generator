{
  "name": "ronasit/laravel-entity-generator",
  "description": "Provided console command for generating entities.",
  "keywords": ["laravel", "entity generator"],
  "license": "MIT",
  "authors": [
    {
      "name": "Roman Dubrovin",
      "email": "rdubrovin@ronasit.com"
    }
  ],
  "require": {
    "php": ">=7.1.3",
    "laravel/framework": ">=5.7.0",
    "ronasit/laravel-helpers": ">=2.0.0-beta",
    "laravel/legacy-factories": ">=1.3.0",
    "ext-json": "*"
  },
  "require-dev": {
    "fzaninotto/faker": "~1.4",
    "mockery/mockery": "^1.4",
    "phpunit/phpunit": "^8.0",
    "mikey179/vfsstream": "^1.6.11",
    "php-mock/php-mock": "^2.4.1",
    "orchestra/testbench": "^6.25"
  },
  "autoload": {
    "psr-4": {
      "RonasIT\\Support\\Tests\\": "tests/",
      "RonasIT\\Support\\": "src/"
    },
    "files": [
      "src/helpers.php"
    ]
  },
  "autoload-dev": {
    "files": [
      "tests/TestCase.php"
    ]
  },
  "minimum-stability": "beta",
  "extra": {
    "laravel": {
      "providers": [
        "RonasIT\\Support\\EntityGeneratorServiceProvider"
      ]
    }
  }
}
